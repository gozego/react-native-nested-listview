{
  "name": "zego-react-native-nested-listview",
  "version": "0.6.1",
  "description": "Nested Listview for React native",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest -u && codecov",
    "lint": "eslint --cache ./src",
    "tslint": "tslint -p .",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "tsc -p ./ --outDir dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjmorant/react-native-nested-listview.git"
  },
  "dependencies": {
    "@types/lodash.isequal": "^4.5.5",
    "lodash.isequal": "^4.5.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "node-gyp": "^8.3.0",
    "nyc": "^15.1.0",
    "react": "17.0.2",
    "react-native": "0.66.1",
    "shortid": "^2.2.16"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-native": "0.66.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.31",
    "@types/react-native": "^0.65.9",
    "@types/react-test-renderer": "^17.0.1",
    "@types/shortid": "^0.0.29",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.3.1",
    "babel-preset-react-native": "^4.0.1",
    "eslint": "^8.0.1",
    "eslint-plugin-flowtype": "^6.1.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-native": "^3.11.0",
    "flow-bin": "^0.162.1",
    "istanbul-api": "^3.0.0",
    "istanbul-reports": "^3.0.5",
    "jest": "^27.3.1",
    "prettier-eslint": "^13.0.0",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.0.7",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  },
  "keywords": [
    "react",
    "native",
    "list",
    "nested"
  ],
  "author": "Javier Morant",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjmorant/react-native-nested-listview/issues"
  },
  "homepage": "https://github.com/fjmorant/react-native-nested-listview#readme",
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)|react-native|react-navigation|react-clone-referenced-element|mobx-react)"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "modulePathIgnorePatterns": [
      "<rootDir>/examples/"
    ],
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/examples/",
      "<rootDir>/lib/",
      "<rootDir>/dist/"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "globals": {
      "__DEV__": true,
      "ts-jest": {
        "babelConfig": true
      }
    }
  }
}
